{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"rc-web": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"inlineTemplate": true,
					"inlineStyle": true
				}
			},
			"root": "",
			"sourceRoot": "src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:browser",
					"options": {
						"outputPath": "dist/rc-web",
						"index": "src/index.html",
						"main": "src/main.ts",
						"polyfills": "src/polyfills.ts",
						"tsConfig": "tsconfig.app.json",
						"aot": true,
						"assets": [
							"src/favicon.ico",
							"src/assets",
							{
								"glob": "./CHANGELOG.md",
								"input": ".",
								"output": "/"
							},
							{
								"glob": "**/*",
								"input": "./node_modules/vscode-icons/icons",
								"output": "./assets/icons"
							},
							{
								"glob": "**/*",
								"input": "./node_modules/ngx-monaco-editor/assets/monaco",
								"output": "./assets/monaco/"
							},
							{
								"glob": "all.svg",
								"input": "./node_modules/css.gg/icons",
								"output": "/"
							}
						],
						"styles": ["src/styles.scss"],
						"scripts": []
					},
					"configurations": {
						"production": {
							"fileReplacements": [
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.prod.ts"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"extractCss": true,
							"namedChunks": false,
							"extractLicenses": true,
							"vendorChunk": true,
							"commonChunk": true,
							"buildOptimizer": true,
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "2mb",
									"maximumError": "5mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "6kb",
									"maximumError": "10kb"
								}
							],
							"aot": true
						}
					}
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"options": {
						"browserTarget": "rc-web:build"
					},
					"configurations": {
						"production": {
							"browserTarget": "rc-web:build:production"
						}
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "rc-web:build"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "src/test.ts",
						"polyfills": "src/polyfills.ts",
						"tsConfig": "tsconfig.spec.json",
						"karmaConfig": "karma.conf.js",
						"assets": [
							"src/favicon.ico",
							"src/assets",
							{
								"glob": "./CHANGELOG.md",
								"input": ".",
								"output": "/"
							},
							{
								"glob": "**/*",
								"input": "./node_modules/vscode-icons/icons",
								"output": "./assets/icons"
							},
							{
								"glob": "**/*",
								"input": "./node_modules/ngx-monaco-editor/assets/monaco",
								"output": "./assets/monaco/"
							},
							{
								"glob": "all.svg",
								"input": "./node_modules/css.gg/icons",
								"output": "/"
							}
						],
						"styles": ["src/styles.scss"],
						"scripts": []
					}
				},
				"lint": {
					"builder": "@angular-eslint/builder:lint",
					"options": {
						"eslintConfig": ".eslintrc.json",
						"tsConfig": ["tsconfig.app.json", "tsconfig.spec.json", "e2e/tsconfig.json"],
						"exclude": ["**/node_modules/**", "e2e/**"]
					}
				},
				"e2e": {
					"builder": "@angular-devkit/build-angular:protractor",
					"options": {
						"protractorConfig": "e2e/protractor.conf.js",
						"devServerTarget": "rc-web:serve"
					},
					"configurations": {
						"production": {
							"devServerTarget": "rc-web:serve:production"
						}
					}
				},
				"deploy": {
					"builder": "angular-cli-ghpages:deploy",
					"options": {}
				}
			}
		}
	},
	"defaultProject": "rc-web"
}
